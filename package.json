    "@aws-sdk/util-dynamodb": "^3.202.0"
  },
  "devDependencies": {
    "@shelf/serverless-simplify-default-exec-role-plugin": "^0.0.2",
    "@types/node": "^18.11.9",
    "aws-lambda": "^1.0.7",
    "jest": "^29.3.1",
    "serverless": "^3.23.0",
    "serverless-offline": "^11.2.3",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
